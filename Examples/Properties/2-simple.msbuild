<?xml version="1.0" encoding="utf-8"?>
<!--
  A very simple msbuild project with a single property and a single target
-->
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <Debug>true</Debug>
  </PropertyGroup>
  <Target Name="Build">
    <Message Text="The value of 'Debug' property is $(Debug)"/>
  </Target>
</Project>